        <activity android:name=".view.ZhiHuListActivity"></activity>
        <activity android:name=".view.ZhiHuPagerActivity"></activity>
        <activity android:name=".view.ZhiHuCommentActivity"></activity>
        <activity android:name=".view.ZhiHuThemeActivity"></activity>
        <service android:name=".service.ZhiHuLatestNewsService"></service>
    </application>
</manifest>
